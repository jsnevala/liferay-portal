	org.apache.cxf.jaxrs.provider,\
	org.apache.cxf.jaxrs.provider.aegis,\
	org.apache.cxf.jaxrs.provider.atom,\
	org.apache.cxf.jaxrs.provider.dom4j,\
	org.apache.cxf.jaxrs.provider.json.utils,\
