definition {
	macro generatePasswordWithAllOptions {
		AssertElementPresent(locator1 = "PasswordGenerator#BLANK_NEW_PASSWORD");

		Check(locator1 = "Checkbox#NUMBERS");

		Check(locator1 = "Checkbox#LOWER_CASE_LETTERS");

		Check(locator1 = "Checkbox#UPPER_CASE_LETTERS");

		Select(locator1 = "Select#LENGTH", value1 = "${passwordLength}");

		AssertClick(locator1 = "Button#GENERATE", value1 = "Generate");

		AssertElementNotPresent(locator1 = "PasswordGenerator#BLANK_NEW_PASSWORD");
	}
}