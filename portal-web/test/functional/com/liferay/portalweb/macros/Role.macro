			else if (contains("${permissionDefinitionKey}", "CONTROL_PANEL_USERS_USER_GROUPS")) {
				var permissionNavigationKey = "CONTROL_PANEL_USERS_USER_GROUPS";
				var permissionNavigationValue = "User Groups";
