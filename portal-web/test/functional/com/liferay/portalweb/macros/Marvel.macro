definition {
	macro viewAsGuestPG {
		AssertElementNotPresent(locator1 = "IFrame#MARVEL");

		Navigator.openSpecificURL(
			url = "http://localhost:5601/o/portal-search-elasticsearch-marvel-web/marvel-proxy/app/kibana"
		);

		AssertTextPresent(
			locator1 = "Home#DIRECT_URL",
			value1 = "{&quot;statusCode&quot;:404,&quot;error&quot;:&quot;Not Found&quot;}"
		);
	}

	macro viewPG {
		SelectFrame(locator1 = "IFrame#MARVEL");

		AssertElementPresent(locator1 = "Marvel#HOME_HEADER");

		AssertElementPresent(locator1 = "Marvel#WELCOME_MESSAGE");

		AssertClick(
			locator1 = "Marvel#CLUSTER_NAME",
			value1 = "LiferayElasticsearchCluster"
		);

		AssertElementPresent(locator1 = "Marvel#CLUSTERS");

		AssertElementPresent(locator1 = "Marvel#INDICES");

		AssertElementPresent(locator1 = "Marvel#NODES");

		AssertElementPresent(locator1 = "Marvel#OVERVIEW");

		AssertElementPresent(locator1 = "Marvel#STATUS_CLUSTER_NAME");

		AssertElementPresent(locator1 = "Marvel#STATUS_CONTAINER");

		AssertElementPresent(locator1 = "Marvel#INDEXING_LATENCY");

		AssertElementPresent(locator1 = "Marvel#INDEXING_RATE");

		AssertElementPresent(locator1 = "Marvel#SEARCH_LATENCY");

		AssertElementPresent(locator1 = "Marvel#SEARCH_RATE");

		AssertElementPresent(locator1 = "Marvel#SHARD_ACTIVITY_HEADER");
	}
}