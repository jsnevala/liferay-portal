	macro addPG {
		AssertClick(locator1 = "Button#POST_NEW_THREAD", value1 = "Post New Thread");

		AssertElementPresent(locator1 = "TextInput#SUBJECT");

		Click(locator1 = "TextInput#SUBJECT");

		Type(locator1 = "TextInput#SUBJECT", value1 = "${threadSubject}");

		if (isSet(source)) {
