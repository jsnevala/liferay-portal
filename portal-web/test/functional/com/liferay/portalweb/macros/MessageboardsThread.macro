				AssertTextEquals(
					locator1 = "CKEditor#TOOLBAR_SIZE_BUTTON_DROPDOWN",
					value1 = "${fontSize}",
					key_fontSize = "${fontSize}"
				);
			}
		}
		else {
			Type.typeCKEditorWaitForCKEditor(
				locator1 = "CKEditor#BODY_FIELD",
				value1 = "${threadBody}"
			);
		}

		PortletEntry.publish();
	}

