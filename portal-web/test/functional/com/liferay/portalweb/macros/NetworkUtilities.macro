definition {
	macro searchPG {
		var key_networkDomain = "${networkDomain}";

		Type.typeAceEditor(
			locator1 = "NetworkUtilities#DNS_LOOKUP_FIELD",
			value1 = "${dnsDomain}"
		);

		AssertClick(locator1 = "Button#SUBMIT_1", value1 = "Search");

		AssertTextEquals.assertPartialText(
			locator1 = "NetworkUtilities#NETWORK_DOMAIN",
			value1 = "${networkDomain}"
		);
	}
}