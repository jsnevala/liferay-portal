definition {
	macro click {
		Click.mouseDownMouseUp(locator1 = "Button#ANY", key_text = "${button}");
	}

	macro clickAddRow {
		Click(locator1 = "Button#ADD_ROW");
	}

	macro clickCancel {
		Button.click(button = "Cancel");
	}

	macro clickConfirm {
		Button.click(button = "Confirm");
	}

	macro clickDone {
		Button.click(button = "Done");
	}

	macro clickInstall {
		Button.click(button = "Install");
	}

	macro clickNext {
		Button.click(button = "Next");
	}

	macro clickPublish {
		ScrollWebElementIntoView(locator1 = "Button#ANY", key_text = "Publish");

		Button.click(button = "Publish");
	}

	macro clickSave {
		Button.click(button = "Save");
	}

	macro clickSaveAsDraft {
		Button.click(button = "Save as Draft");
	}

	macro clickSubmitForPublication {
		Button.click(button = "Submit for Publication");
	}

	macro clickUpdate {
		Button.click(button = "Update");
	}

	macro viewNotPresent {
		AssertElementNotPresent(locator1 = "Button#ANY", key_text = "${button}");
	}
}