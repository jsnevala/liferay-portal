		for (var webContentTitle : list "Web Content Title 1,Web Content Title 2") {
			ContentTargetingUserSegment.addBehaviorContentVisitedRuleCP(
				contentVisitedContent = "Basic Web Content",
				webContentTitle = "${webContentTitle}"
