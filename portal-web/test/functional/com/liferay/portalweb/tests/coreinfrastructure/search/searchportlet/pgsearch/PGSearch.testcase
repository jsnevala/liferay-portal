@component-name = "portal-search"
definition {
	property portal.release = "true";
	property portal.suite.search.engine = "elasticsearch2,elasticsearch6,solr";
	property portal.upstream = "true";
	property testray.main.component.name = "Web Search";
	var searchPageName = "Test Page";
	var searchPortletName = "Search";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();

		Page.add(pageName = "Test Page");
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			BlogsEntry.tearDownCP();

			MessageboardsThread.tearDownCP();

			User.tearDownCP();

			WebContent.tearDownCP();

			WikiPage.tearDownCP();

			Vocabulary.tearDownCP();

			Tag.tearDownCP();

			Page.tearDownPG();
		}
	}

	@priority = "4"
