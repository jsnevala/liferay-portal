		property custom.properties = "permissions.check.guest.enabled=false";

		ProductMenu.gotoPortlet(
			category = "Sites",
			panel = "Control Panel",
			portlet = "Sites"
		);

		Site.addBlankCP(siteName = "Site Name");

		Page.add(
			pageName = "Private Blogs Page",
			siteName = "Site Name",
			sitePagePrivacy = "Private"
		);

		ProductMenuHelper.expandCategory(category = "Navigation");

		PermissionsInline.gotoSitepageCP(
			page = "Private Pages",
			pageName = "Private Blogs Page",
			siteName = "Site Name"
		);

		AssertTextNotPresent(
			locator1 = "Permissions#CONTENT_PERMISSIONS_ROLE_NAME",
			value1 = "Guest",
			key_roleName = "Guest"
		);

		Navigator.openPrivateSitePageURL(
			pageName = "Private Blogs Page",
			siteName = "Site Name"
		);

		Portlet.addPG(portletName = "Blogs");

		PermissionsInline.gotoConfigurationPG(portletName = "Blogs");

		AssertTextNotPresent(
			locator1 = "Permissions#CONTENT_PERMISSIONS_ROLE_NAME",
			value1 = "Guest",
			key_roleName = "Guest"
		);
	}

