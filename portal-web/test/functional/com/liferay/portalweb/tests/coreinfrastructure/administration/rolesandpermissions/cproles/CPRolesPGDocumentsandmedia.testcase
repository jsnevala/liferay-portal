@component-name = "portal-permissions"
definition {
	property portal.release = "true";
	property portal.upstream = "true";
	property testray.main.component.name = "Permissions";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			User.firstLoginPG();

			User.tearDownCP();

			Role.tearDownCP();

			Organization.tearDownCP();

			Site.tearDownCP();

			Page.tearDownPG();

			DMFolder.tearDownCP();

			DMDocument.tearDownCP();
		}
	}

	@priority = "5"
	test AddDMDocumentRegRoleDefaultPermissions {
		property portal.acceptance = "false";

		Permissions.setUpRegRolePG(
			pageName = "Documents and Media Page",
			portletName = "Documents and Media",
			roleName = "Regrole Name",
			userEmailAddress = "userea@liferay.com",
			userFirstName = "userfn",
			userLastName = "userln",
			userScreenName = "usersn"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "userea@liferay.com",
			userLoginFullName = "userfn userln"
		);

		Navigator.gotoPage(pageName = "Documents and Media Page");

		AssertElementNotPresent(locator1 = "Toolbar#ADD");
	}

	@priority = "5"
	test AddDMDocumentRegRoleDefinePermissions {
		property portal.acceptance = "false";

		Permissions.setUpRegRolePG(
			pageName = "Documents and Media Page",
			portletName = "Documents and Media",
			roleName = "Regrole Name",
			userEmailAddress = "userea@liferay.com",
			userFirstName = "userfn",
			userLastName = "userln",
			userScreenName = "usersn"
		);

		Permissions.definePermissionCP(
			permissionDefinitionKey = "SITE_ADMIN_CONTENT_DOCUMENTS_AND_MEDIA_RESOURCE_PERMISSIONS_DOCUMENTS_ADD_DOCUMENT_CHECKBOX",
			permissionDefinitionValue = "Add Document",
			roleName = "Regrole Name"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "userea@liferay.com",
			userLoginFullName = "userfn userln"
		);

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.addPG(
			dmDocumentFile = "Document_1.doc",
			dmDocumentTitle = "Document_1.doc"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "test@liferay.com",
			userLoginFullName = "Test Test"
		);

		Permissions.removePermissionCP(
			permissionDefinitionKey = "SITE_ADMIN_CONTENT_DOCUMENTS_AND_MEDIA_RESOURCE_PERMISSIONS_DOCUMENTS_ADD_DOCUMENT_CHECKBOX",
			permissionDefinitionValue = "Add Document",
			roleName = "Regrole Name"
		);

		User.logoutAndLoginPG(
			userLoginEmailAddress = "userea@liferay.com",
			userLoginFullName = "userfn userln"
		);

		Navigator.gotoPage(pageName = "Documents and Media Page");

		AssertElementNotPresent(locator1 = "Toolbar#ADD");
	}

