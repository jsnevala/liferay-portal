@component-name = "portal-plugins-osgi"
definition {
	property portal.release = "true";
	property portal.upstream = "true";
	property plugins.deployment.type = "osgi";
	property testray.main.component.name = "User Interface";

	setUp {
		User.firstLoginPG();
	}

	@priority = "4"
	test 212ColumnsLayouttplSmoke {
		property layouttpl.plugins.includes = "2-1-2-columns-layouttpl";
		var appName = "2-1-2 Columns Layout";
		var portletName = "2-1-2-columns-layouttpl";

		OSGiSmoke.Smoke(appName = "${portletName}", portletName = "${portletName}");
	}
}