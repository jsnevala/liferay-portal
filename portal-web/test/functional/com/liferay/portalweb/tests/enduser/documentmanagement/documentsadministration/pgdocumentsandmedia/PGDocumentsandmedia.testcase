	@priority = "5"
	test EditAndDeleteGoogleDoc {
		property osgi.app.includes = "document-library-google-docs";
		property plugins.deployment.type = "osgi";
		property portal.release = "false";
		property test.name.skip.portal.instance = "PGDocumentsandmedia#EditAndDeleteGoogleDoc";

		var siteName = TestCase.getSiteName(siteName = "${siteName}");

		ProductMenu.gotoPortlet(
			category = "Configuration",
			panel = "Control Panel",
			portlet = "Instance Settings"
		);

		Navigator.gotoNavTab(navTab = "Miscellaneous");

		var googlePropertiesSet = MathUtil.randomNumber("3");
		var googleAppsAPIKey = PropsUtil.get("google.api.key.${googlePropertiesSet}");
		var googleClientID = PropsUtil.get("google.client.id.${googlePropertiesSet}");

		PortalSettings.configureGoogleApps(
			googleAppsAPIKey = "${googleAppsAPIKey}",
			googleClientID = "${googleClientID}"
		);

		Navigator.gotoPage(pageName = "Documents and Media Page");

		Portlet.clickPortletPlusIconPG(portletName = "Documents and Media");

		MenuItem.click(menuItem = "Google Docs");

		GoogleDoc.gotoDrive(
			userEmailAddress = PropsUtil.get("email.address.3"),
			userPassword = PropsUtil.get("email.password.3"),
			windowTitle = "Documents and Media Page - ${siteName}"
		);

		GoogleDoc.selectDocument(googleDocName = "taco.jpg");

		PortletEntry.publish();

		DMDocument.viewPG(dmDocumentTitle = "taco.jpg");

		Navigator.gotoPage(pageName = "Documents and Media Page");

		DMDocument.deletePG(dmDocumentTitle = "taco.jpg");
	}

