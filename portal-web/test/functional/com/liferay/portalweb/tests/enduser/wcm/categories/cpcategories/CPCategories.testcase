		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Content",
			panel = "Site Administration",
			portlet = "Categories"
		);

		VocabularyNavigator.gotoAdd();

		PortletEntry.inputName(name = "Vocabulary Name");

		Vocabulary.addAssociatedAssetType(assetType = "Blogs Entry", required = "true");

		Button.clickAddRow();

		Vocabulary.addAssociatedAssetType(
			assetType = "Web Content Article",
			required = "true",
			rowNumber = "2"
		);

		Button.clickSave();

		Vocabulary.deleteAssociatedAssetTypeViaVocabularyCP(
			vocabularyName = "Vocabulary Name"
		);

		Vocabulary.viewWithAssociatedAssetTypeCP(
			vocabularyAssociatedAssetType = "Blogs Entry",
			vocabularyName = "Vocabulary Name"
		);
	}

