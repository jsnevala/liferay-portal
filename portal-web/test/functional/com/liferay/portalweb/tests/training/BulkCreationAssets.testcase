@component-name = "portal-training"
definition {
	property liferay.training.dependency.jars = "tools/dummy-factory/liferay.dummy.factory-0.17.0.jar";
	property portal.release = "false";
	property portal.upstream = "false";
	property testray.main.component.name = "Site Administration";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			User.tearDownCP();

			Site.tearDownCP();

			Organization.tearDownCP();

			Team.tearDownCP();

			Page.tearDownPG();

			UserGroup.tearDownCP();
		}
	}

	@priority = "5"
	test AddBlogsWithDummyFactory {
		property liferay.training.dependency.jars = "tools/dummy-factory/liferay.dummy.factory-0.17.0.jar";
		property test.name.skip.portal.instance = "BulkCreationAssets#AddBlogsWithDummyFactory";
		property testray.component.names = "Training";
		property timeout.explicit.wait = "600";

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "App",
			panel = "Control Panel",
			portlet = "Dummy Factory"
		);

		DummyFactory.addBulkAssets(
			baseName = "Blog Title",
			contents = "Blog contents",
			dateType = "Blogs",
			number = "500"
		);

		ProductMenu.gotoPortlet(
			category = "Content",
			panel = "Site Administration",
			portlet = "Blogs"
		);

		Portlet.changeDisplayStyle(displayStyle = "List");

		AssertTextEquals.assertPartialText(
			locator1 = "Blogs#ENTRY_TABLE_TITLE",
			value1 = "Blog Title",
			key_entryTitle = "Blog Title"
		);
	}

