@default = "select"
definition {
	function pauseSelect {
		WaitForSPARefresh();

		selenium.pause("5000");

		Select.select();

		selenium.assertJavaScriptErrors();

		selenium.assertLiferayErrors();
	}

	function select {
		WaitForSPARefresh();

		selenium.waitForVisible();

		selenium.mouseOver();

		selenium.select();

		selenium.assertJavaScriptErrors();

		selenium.assertLiferayErrors();
	}
}